<h1>Registration</h1>
<form action="<%= user_url %>" method="post">
  <input type="hidden"
  name="authenticity_token"
  value="<%= form_authenticity_token %>">

  <input type="text"
  name="user[user_name]"
  value="<%= @user.user_name %>"
  placeholder="username">

  <input type="password"
  name="user[password]"
  placeholder="password">
  <input type="submit" value="Create User">

</form>
